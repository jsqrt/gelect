{
  "author": "Glivera-team",
  "name": "glivera-webpack-template",
  "version": "1.0.0",
  "description": "Webpack 5, pug, scss, svg sprite template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config settings/webpack.dev.config.js --mode=development",
    "dev-build": "cross-env NODE_ENV=development webpack --config settings/webpack.dev.config.js --mode=development --watch",
    "build": "cross-env NODE_ENV=production webpack --config settings/webpack.prod.config.js --mode=production",
    "wp-build": "webpack --config settings/webpack.wp_prod.config.js",
    "stats": "webpack --config settings/webpack.prod.config.js --mode=production --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@squoosh/lib": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^5.0.11",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.2.2",
    "image-webpack-loader": "^8.1.0",
    "imagemin": "^8.0.1",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "mini-css-extract-plugin": "2.4.5",
    "portfinder-sync": "0.0.2",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.43.4",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "gsap": "^3.9.1",
    "ismobilejs": "^1.1.1",
    "jquery": "^2.2.4"
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.4.5"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "engines": {
    "node": "^12 || >=14"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup",
    "scss",
    "pug",
    "svg sprite "
  ],
  "target": "web"
}
